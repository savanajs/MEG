{
  "name": "meg",
  "version": "0.0.2",
  "description": "Make more layouts, write less css",
  "public_path": "",
  "private": true,
  "browserslist": [
    "> 10%",
    "ie > 9",
    "last 5 version"
  ],
  "config": {
    "html_params_hash": false,
    "has_pwa": true
  },
  "is_style_loader": false,
  "minify": {
    "html": true,
    "exclude": "src/docs.html"
  },
  "pwa_manifest": {
    "name": "MEG CSS Framework",
    "short_name": "MEG",
    "description": "Make more layouts, write less css",
    "background_color": "#f5f5f5",
    "theme_color": "#00cc99"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "docs": "gulp setFileCssDarkName && webpack -p && gulp getCSSMINAndRenameToDark &&  gulp setFileCssDefaultName && webpack -p && gulp default",
    "start": "webpack-dev-server --content-base src --watch --open --progress --colors --inline --mode development --port 3000"
  },
  "keywords": [
    "framework",
    "css"
  ],
  "author": "Rafael Cruz",
  "license": "The MIT License (MIT)",
  "entry": "js",
  "devDependencies": {
    "autoprefixer": "^8.6.3",
    "autoprefixer-loader": "^3.2.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "clean-webpack-plugin": "^0.1.19",
    "css-loader": "^0.28.11",
    "eslint": "^5.0.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-standard": "^3.1.0",
    "file-loader": "^1.1.11",
    "gulp": "^3.9.1",
    "gulp-clean": "^0.4.0",
    "gulp-image": "^4.4.1",
    "gulp-rename": "^1.4.0",
    "gulp-replace": "^1.0.0",
    "gulp-rimraf": "^0.2.2",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "html-webpack-plugin-template-vars": "^2.28.2",
    "htmllint-loader": "^2.1.4",
    "image-webpack-loader": "^4.3.1",
    "inject-variable-html-webpack-plugin": "^1.0.2",
    "jquery": "^3.3.1",
    "mini-css-extract-plugin": "^0.4.0",
    "node-sass": "^4.9.0",
    "optimize-css-assets-webpack-plugin": "^4.0.2",
    "postcss-loader": "^2.1.5",
    "run-sequence": "^2.2.1",
    "sass-loader": "^7.0.3",
    "style-loader": "^0.21.0",
    "stylelint": "^9.3.0",
    "stylelint-webpack-plugin": "^0.10.5",
    "sw-precache-webpack-plugin": "^0.11.5",
    "ts-loader": "^4.4.1",
    "typescript": "^2.9.2",
    "uglifyjs-webpack-plugin": "^1.2.6",
    "webpack": "^4.12.0",
    "webpack-cli": "^3.0.8",
    "webpack-concat-plugin": "^3.0.0",
    "webpack-dev-server": "^3.1.4",
    "webpack-livereload-plugin": "^2.2.0",
    "webpack-pwa-manifest": "^3.7.1"
  },
  "dependencies": {
    "handlebars": "^4.0.11",
    "handlebars-loader": "^1.7.0",
    "prismjs": "^1.15.0"
  }
}


{
  "env": {
    "browser": true,
    "es6": true 
  },
  "parserOptions": {
    "sourceType": "module"
  },
  "extends": "eslint:recommended",
  "rules": {
    "wrap-iife": 0,
    "func-names": 0,
    "space-before-function-paren": 0,
    "space-before-blocks": 0,
    "no-undef": 0,
    "indent": [
      0
    ],
    "id-length": 0,
    "prefer-arrow-callback": 0,
    "max-statements": 0,
    "no-magic-numbers": 0,
    "quotes": [
      0,
      "double"
    ],
    "max-len": 0,
    "no-var": 1,
    "vars-on-top": 0,
    "no-unused-vars": 2,
    "one-var": 0,
    "prefer-const": 1,
    "init-declarations": 0,
    "no-console": 1,
    "no-mixed-spaces-and-tabs": 1,
    "no-trailing-spaces": 0,
    "padded-blocks": 0,
    "curly": 0,
    "no-empty-func": 0,
    "no-empty-function": 0,
    "no-empty": 0,
    "brace-style": 0,
    "no-label-var": 2,
    "func-style": 0,
    "require-jsdoc": [
      0,
      {
        "require": {
          "FunctionDeclaration": true,
          "MethodDefinition": true,
          "ClassDeclaration": true
        }
      }
    ],
    "no-extra-semi": 2,
    "no-plusplus": 0,
    "object-curly-spacing": 0,
    "dot-notation": 1,
    "quote-props": 0,
    "eol-last": 0,
    "semi": 0,
    "no-alert": 0,
    "space-in-parens": 2,
    "linebreak-style": [
      0,
      "unix"
    ],
    "camelcase": 0,
    "comma-spacing": 2,
    "sort-keys": 0,
    "object-shorthand": 0,
    "prefer-template": 0,
    "no-array-constructor": 0,
    "keyword-spacing": 2,
    "comma-dangle": 0,
    "strict": 1,
    "newline-after-var": 2,
    "newline-before-return": 0,
    "newline-per-chained-call": 0,
    "object-property-newline": 2,
    "object-curly-newline": 1,
    "dot-location": 0,
    "key-spacing": [
      2,
      {
        "beforeColon": false,
        "afterColon": true
      }
    ]
  }
}
